{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"title": "task result",
	"description": "...",
	"type": "object",
	"properties": {
		"id": { 
			"type": "string",
			"pattern": "^[0-9a-f]+$" 
		},
		"execution_time": {
			"type": "number"
		},
		"start_date": {
			"type": "string",
			"format": "date"
		},
		"returnValue": { 
			"oneof": {
				"type": "array",
				"type": "object"
			}
		}
	},
	"required": ["id", "returnValue"]
}
